PROGRAM = axis

CC = g++

ARCH = i686

BUILD_STRING=LINUX

CFLAGS_T = -Wall -march=$(ARCH) -mtune=$(ARCH) -m32 -O2 -fstack-protector-all -Wno-unused-result -static -s

ifdef HAVE_HASH_TBL
  CFLAGS = $(CFLAGS_T) -DHAVE_HASH_TBL=1
else
  CFLAGS = $(CFLAGS_T)
endif

INCLUDES =  -I.
LDFLAGS = -Wall -march=$(ARCH) -mtune=$(ARCH) -m32 -O2 -fstack-protector-all -static -s
LFLAGS = -lpthread -lrt
EXEEXT =
